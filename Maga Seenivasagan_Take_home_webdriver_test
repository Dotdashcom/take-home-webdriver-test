4.Context Menu

Public class Test4{

  Public static void main(String [] args){

    System.Set property("Webdriver.chrome.driver,"system path");

     WebDriver driver = new chromeDriver();

     driver.get("http://localhost:7080/context_menu");
  
     driver.manage().window.maximize();

     WebElement img1= driver.findElement(By.locators);

     Actions ac= new Actions(driver);
     
      ac.contextclick(img1).perform();

          driver.close();

 }
}


5.Drag and Drop

Public class Test5{

  Public static void main(String [] args){

    System.Set property("Webdriver.chrome.driver,"system path");

     WebDriver driver = new chromeDriver();

     driver.get("http://localhost:7080/drag_and_drop");
  
     driver.manage().window.maximize();

     WebElement elementA= driver.findElement(By.locators);
 
     WebElement elementB= driver.findElement(By.locators);


     Actions ac= new Actions(driver);
     
      ac.dragAndDrop(elementA,elementB).perform();

          driver.close();

 }
}


6.Dropdown


Public class Test6{

  Public static void main(String [] args){

    System.Set property("Webdriver.chrome.driver,"system path");

     WebDriver driver = new chromeDriver();

     driver.get("http://localhost:7080/dropdown");
  
     driver.manage().window.maximize();

     WebElement dropvalue= driver.findElement(By.locators);
     
       Select select = new Select(dropvalue)

      select.selectByVisibleText("Option 1");
    
       select.selectByVisibleText("Option 2");

          driver.close();

 }
}


7.Dynamic Content

Right click refresh

Public class Test7{

  Public static void main(String [] args){

    System.Set property("Webdriver.chrome.driver,"system path");

     WebDriver driver = new chromeDriver();

     driver.get("http://localhost:7080/dynamic_content");
  
     driver.manage().window.maximize();

     WebElement userN= driver.findElement(By.locators);

     Actions ac= new Actions(driver);
     
      ac.contextclick(userN).perform();

       Robert rb = new Robert();

       rb.keyPress(KeyEvent.VK_DOWN);

       rb.keyPress(KeyEvent.VK_DOWN);

       rb.keyPress(KeyEvent.VK_ENTER);


       ac.contextclick(userN).perform();

       Robert rb = new Robert();

       rb.keyPress(KeyEvent.VK_DOWN);

       rb.keyPress(KeyEvent.VK_DOWN);

       rb.keyPress(KeyEvent.VK_ENTER);

       driver.close();

 }
}


8.Dynamic Controls

Public class Test9{

  Public static void main(String [] args){

    System.Set property("Webdriver.chrome.driver,"system path");

     WebDriver driver = new chromeDriver();

     driver.get("http://localhost:7080/dynamic_controls");
  
     driver.manage().window.maximize();

     WebElement removeBtn = driver.findElement(By.locators);

      removeBtn.click();

      WebDriverwait wait1 = new WebDriverwait(driver, 10);

         wait1.until(element ToBe Clickable());

     WebElement addBtn = driver.findElement(By.locators);

      addBtn.click();

         wait1.until(element ToBe Clickable());

      WebElement enableBtn = driver.findElement(By.locators);

      enableBtn.click();

         wait1.until(element ToBe Clickable());

       WebElement disableBtn = driver.findElement(By.locators);

      disableBtn.click();

         wait1.until(element ToBe Clickable());

          driver.close();

 }
}


