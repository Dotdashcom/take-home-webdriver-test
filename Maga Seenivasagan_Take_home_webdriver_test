1.Login Success

Public class Test1{

  Public static void main(String [] args){

    System.Set property("Webdriver.chrome.driver,"system path");

     WebDriver driver = new chromeDriver();

     driver.get("http://localhost:7080/login");
  
     driver.manage().window.maximize();

     WebElement txtusername= driver.findElement(By.locators);
     
         txtusername.sendkeys("tomsmith");

     WebElement txtpassword= driver.findElement(By.locators);
     
         txtpassword.sendkeys("SuperSecretPassword!");

    WebElement txtlogin= driver.findElement(By.locators);
     
         txtlogin.click();

          driver.close();

 }
}

 
