11.File Upload
   
Public class Test11{

  Public static void main(String [] args){

    System.Set property("Webdriver.chrome.driver,"system path");

     WebDriver driver = new chromeDriver();

     driver.get("http://localhost:7080/upload");
  
     driver.manage().window.maximize();

     WebElement addUploadFile = driver.findElement(By.locators);

     addUploadFile.sendKeys("/Users/system/Desktop/c1.jpeg");

     driver.findElement(By.name("send")).click();

 }
}




12.Floating Menu

Scroll the page

Public class Test12{

  Public static void main(String [] args){

    System.Set property("Webdriver.chrome.driver,"system path");

     WebDriver driver = new chromeDriver();

     driver.get("http://localhost:7080/floating_menu");
  
     driver.manage().window.maximize();

     WebElement scrollUp = driver.findElement(By.locators);

     WebElement scrollDown = driver.findElement(By.locators);

    JavascriptExecutor js = (Javascript Executor) driver;

    js.executeScript("arguments[0].scrollIntoView(true)",scrolldown);

    Thread.sleep(2000);

    js.executeScript("arguments[0].scrollIntoView(false)",scrollup);

      driver.close();

 }
}


13.Iframe

Public class Test13{

  Public static void main(String [] args){

    System.Set property("Webdriver.chrome.driver,"system path");

     WebDriver driver = new chromeDriver();

     driver.get("http://localhost:7080/iframe");
  
     driver.manage().window.maximize();
   
     driver.switchTo().frame("Test iframepage")

     WebElement txtusername= driver.findElement(By.locators);

        txtusername.sendkeys("Hello");
 
           driver.close();

 }
}


14.Mouse Hover

Public class Test14{

  Public static void main(String [] args){

    System.Set property("Webdriver.chrome.driver,"system path");

     WebDriver driver = new chromeDriver();

     driver.get("http://localhost:7080/hovers");
  
     driver.manage().window.maximize();

     WebElement img1= driver.findElement(By.locators);

     Actions ac= new Actions(driver);
     
      ac.moveToElement(img1).perform();

     WebElement img2= driver.findElement(By.locators);
     
        ac.moveToElement(img2).perform();

    WebElement img3= driver.findElement(By.locators);
     
         img.click();

          driver.close();

 }
}

15.JavaScript Alerts

Public class Test15{

  Public static void main(String [] args){

    System.Set property("Webdriver.chrome.driver,"system path");

     WebDriver driver = new chromeDriver();

     driver.get("http://localhost:7080/javascript_alerts");
  
     driver.manage().window.maximize();

     WebElement alertBtn= driver.findElement(By.locators);
  
         alertBtn.click();

         Alert al = driver.switchTo().alert();
 
          al.accept();

    WebElement confirmBtn = driver.findElement(By.locators);
  
         confirmBtn.click();
 
         al.accept();

    WebElement PromptBtn = driver.findElement(By.locators);
  
         PromptBtn.click();

          al.sendkeys("Hello");
 
           al.accept();

             driver.close();

 }
}


